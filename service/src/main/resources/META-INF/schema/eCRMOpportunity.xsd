						<xsd:element name="Doc" type="xsd:base64Binary" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>The actual document</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CurrentChunk">
							<xsd:annotation>
								<xsd:documentation>Current chunk of file transfer when using multichunks
                           to send a file. Number is based on 1 based index.</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:int">
									<xsd:minInclusive value="0"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="TotalChunks">
							<xsd:annotation>
								<xsd:documentation>Total number of chunks to send entire file. Number is
                           inclusive and 1 based index.</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:int">
									<xsd:minInclusive value="0"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
