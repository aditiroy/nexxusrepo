							<xsd:annotation>
								<xsd:documentation>in bytes</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:long"/>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="FileType" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>document extension</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="CreatedByHRID" type="xsd:string">
							<xsd:annotation>
								<xsd:documentation>AE Full Name</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
